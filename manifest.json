{
  "manifest_version": 3,
  "name": "Regex Redirect",
  "description": "Detect regex patterns in any URL and redirect to another URL.",
  "version": "1.0",
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "images/icon.png"
  },
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "host_permissions": [
    "https://developer.chrome.com/*"
  ]
}